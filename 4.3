import cv2 as cv
import numpy as np
image1=cv2.imread('image1.png')
cv2.imshow("dx" ,image1)
cv2.waitKey()
cv2.destroyAllWindows()
image2=cv2.imread('image2.png')
cv2.imshow("d2" ,image2)
cv2.waitKey()
cv2.destroyAllWindows()
pic=np.hstack([image1,image2])
cv2.imshow("collage" ,pic)
cv2.waitKey()
cv2.destroyAllWindows()
